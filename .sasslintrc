{
   "options": {
       "merge-default-rules": true,
       "verbose-success": true
   },
   "rules": {
       "empty-line-between-blocks": 0,
       "final-newline": 1,
       "force-attribute-nesting": 0,
       "indentation": [
           1,
           {
               "size": 4
           }
       ],
       "leading-zero": 0,
       "nesting-depth": 0,
       "no-ids": 0,
       "no-important": 0,
       "no-qualifying-elements": 0,
       "no-vendor-prefixes": 0,
       "shorthand-values": 0,
       "space-after-comma": 0
   }
}
